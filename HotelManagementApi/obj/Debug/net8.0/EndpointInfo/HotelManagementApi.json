    "/api/room/CreateRoom": {
      "post": {
        "tags": [
          "Rooms"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateRoomDto"
              }
            },
            "text/json": {
              "schema": {
